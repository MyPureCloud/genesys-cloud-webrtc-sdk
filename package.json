{
  "name": "purecloud-webrtc-sdk",
  "version": "3.4.0",
  "description": "client for the interfacing with PureCloud WebRTC",
  "repository": "https://github.com/mypurecloud/purecloud-webrtc-sdk",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/purecloud-webrtc-sdk.min.js",
  "browser": "dist/purecloud-webrtc-sdk.min.js",
  "files": [
    "dist"
  ],
  "typings": "dist/src/client.d.ts",
  "pre-push": [
    "test"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p . && webpack && webpack --env.production",
    "build-sample": "./test/test-pages/build.sh",
    "build:sample": "./test/test-pages/build.sh",
    "build-run-local": "concurrently \"./test/test-pages/build-dev.sh\" \"npm start\"",
    "pretest": "npm run build",
    "start": "stupid-server -s -p 8443",
    "start:sample": "npm run build-run-local",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "NODE_ENV=test jest --runInBand",
    "test:watch": "jest --watch --collectCoverage=false --runInBand",
    "lint": "npm run semistandard && npm run tslint",
    "lint:fix": "npm run semistandard:fix && npm run tslint:fix",
    "tslint": "tslint --project . --config tslint.json",
    "tslint:fix": "tslint --project . --config tslint.json --fix",
    "semistandard": "semistandard",
    "semistandard:fix": "semistandard --fix",
    "greenkeep": "npx npm-check --update",
    "codecov": "codecov"
  },
  "semistandard": {
    "ignore": [
      "/.coverage",
      "/dist",
      "**/node_modules",
      "/test/test-pages/sdk-sample.js"
    ]
  },
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "backoff-web": "^1.0.1",
    "browserama": "^3.1.1",
    "lodash": "^4.17.15",
    "purecloud-platform-client-v2": "^73.0.0",
    "purecloud-streaming-client": "^11.2.0",
    "safe-json-stringify": "^1.2.0",
    "superagent": "^5.1.0",
    "uuid": "^3.3.2",
    "webrtc-stats-gatherer": "^7.1.1",
    "wildemitter": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.5.5",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.149",
    "@types/nock": "^11.1.0",
    "@types/node": "^13.9.5",
    "@types/safe-json-stringify": "^1.1.0",
    "@types/superagent": "^4.1.3",
    "@types/uuid": "^7.0.3",
    "@types/webrtc": "0.0.25",
    "@types/ws": "^6.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-preset-env": "^1.7.0",
    "babelify": "^10.0.0",
    "browserify": "^16.3.0",
    "codecov": "^3.6.5",
    "concurrently": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "jest": "^24.8.0",
    "nock": "^10.0.6",
    "pre-push": "^0.1.1",
    "rimraf": "^3.0.0",
    "semistandard": "^13.0.1",
    "stupid-server": "^0.2.5",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.2.1",
    "tslint": "^5.18.0",
    "tslint-config-semistandard": "^8.0.1",
    "typescript": "~3.6.5",
    "uglify-js": "^3.6.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watch": "^1.0.2",
    "webpack": "^4.35.3",
    "webpack-auto-inject-version": "^1.2.2",
    "webpack-cli": "^3.3.5",
    "ws": "^7.1.0"
  },
  "false": {}
}
